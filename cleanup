find . -name \*~ -exec rm {} \;
find . -name \*.o -exec rm {} \;
find . -name \*.so -exec rm {} \;
find . -name "#*#" -exec rm {} \;

# copy wrapper
rm -rf ./inst/wrapper
mkdir ./inst/wrapper
cp wrapper/* ./inst/wrapper

# make wrapper available for automatic checking too
rm -rf ./tests/wrapper
mkdir ./tests/wrapper
cp -R wrapper/* ./tests/wrapper

# copy OS-specific tests
rm -rf ./inst/tests
mkdir ./inst/tests
cp -R tests/testWrappers ./inst/tests

